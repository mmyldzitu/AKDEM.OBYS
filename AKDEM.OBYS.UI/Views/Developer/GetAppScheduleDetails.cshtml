@model List<AppScheduleDetailListDtoDeveloper>

@{
    Layout = "_Layout";
}
<table class="mt-3 table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Day</th>

            <th>Hours</th>
            
            <th>lessonId</th>
            <th>AppScheduleId</th>


            <th>Git</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.Day</td>
                <td>@item.Hours</td>
                <td>@item.LessonId</td>


                <td>@item.ApScheduleId</td>
                

                <td><a href="#">Git</a></td>





            </tr>
        }
    </tbody>

</table>