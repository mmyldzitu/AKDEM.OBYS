@model List<AppSessionBranchListDtoDeveloper>

@{
    Layout = "_Layout";
}
<div class="col-md-2 align-items-center">
    <a asp-action="CreateAppSessionBranch" asp-controller="Developer"  class="mt-2 btn btn-success ">Yeni  Ekle</a>

</div>
<br />
<table class="mt-3 table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>SessionId</th>

            <th>BranchId</th>



            <th>Git</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.SessionId</td>
                <td>@item.BranchId</td>


                <td><a asp-action="UpdateAppSessionBranch" asp-controller="Developer" asp-route-id="@item.Id">Git</a></td>





            </tr>
        }
    </tbody>

</table>