@model List<AppSessionListDto>

@{
    Layout = "_Layout";
}
<table class="mt-3 table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Definition</th>
            <th>MinAverageNote</th>
            <th>MinLessonNote</th>
            <th>MinAbsenteism</th>
            <th>Status</th>
            <th>Status2</th>

            <th>SessionPresident</th>



            <th>Git</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.Definition</td>
                <td>@item.MinAverageNote</td>
                <td>@item.MinLessonNote</td>
                <td>@item.MinAbsenteeism</td>
                <td>@item.Status</td>
                <td>@item.Status2</td>
                <td>@item.Sessionpresident</td>


                <td><a href="#">Git</a></td>





            </tr>
        }
    </tbody>

</table>