@model List<AppGraduatedListDto>
@{
    Layout = "_Layout";
}
<table class="mt-3 table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>President</th>

            <th>year</th>
            <th>UserId</th>
            <th>studentName</th>
            <th>belgeNo</th>


            <th>Git</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.President</td>
                <td>@item.year</td>
                <td>@item.UserId</td>


                <td>@item.studentName</td>
                <td>@item.belgeNo</td>

                <td><a href="#">Git</a></td>





            </tr>
        }
    </tbody>

</table>
