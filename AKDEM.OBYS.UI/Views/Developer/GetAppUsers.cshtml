@model List<AppUserListDtoDeveloper>

@{
    Layout = "_Layout";
}
<table class="mt-3 table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>FirstName</th>
            <th>SecondName</th>
            <th>Email</th>
            <th>Password</th>
            <th>PhoneNumber</th>
            <th>Status</th>

            <th>ImagePath</th>
            <th>TotalAverage</th>
            <th>SıraNo</th>
            <th>DepartReason</th>
            <th>TotalWarningCount</th>
            <th>BranchId</th>
            <th>ClassId</th>



            <th>Git</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.SecondName</td>
                <td>@item.Email</td>
                <td>@item.Password</td>
                <td>@item.PhoneNumber</td>
                <td>@item.Status</td>
                <td>@item.ImagePath</td>
                <td>@item.TotalAverage</td>
                <td>@item.SıraNo</td>
                <td>@item.DepartReason</td>
                <td>@item.TotalWarningCount</td>
                <td>@item.BranchId</td>
                <td>@item.ClassId</td>


                <td><a asp-action="UpdateAppUser" asp-controller="Developer" asp-route-id="@item.Id">Git</a></td>





            </tr>
        }
    </tbody>

</table>